<%= render("./partials/header/header") %>
<!--The main content-->
<div class="shell">
  <p id="notice"><%= notice %></p>
  <h1>Follow Stock Newsletters</h1>
  <table>
    <thead>
      <tr>
        <th>Stock</th>
        <th>Isin</th>
        <th>Email</th>
        <th>Active</th>
        <th>Privacy</th>
        <th>Referrer</th>
        <th>Sended</th>
        <th>Total sended</th>
        <th>Code</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @follow_stock_newsletters.each do |follow_stock_newsletter| %>
        <tr>
          <td><%= follow_stock_newsletter.stock %></td>
          <td><%= follow_stock_newsletter.isin %></td>
          <td><%= follow_stock_newsletter.email %></td>
          <td><%= follow_stock_newsletter.active %></td>
          <td><%= follow_stock_newsletter.privacy %></td>
          <td><%= follow_stock_newsletter.referrer %></td>
          <td><%= follow_stock_newsletter.sended %></td>
          <td><%= follow_stock_newsletter.total_sended %></td>
          <td><%= follow_stock_newsletter.code %></td>
          <td><%= link_to 'Show', follow_stock_newsletter %></td>
          <td><%= link_to 'Edit', edit_follow_stock_newsletter_path(follow_stock_newsletter) %></td>
          <td><%= link_to 'Destroy', follow_stock_newsletter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Follow Stock Newsletter', new_follow_stock_newsletter_path %>
</div>
